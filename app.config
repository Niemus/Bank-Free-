<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="Банк_Свобода.My.MySettings.Microsoft_Office_Access_2007_База_данных__2_ConnectionString"
